{
  "name": "syradev/html5_patch",
  "type": "typo3-cms-extension",
  "description": "This extension patches TYPO3 to eliminate non-HTML5-standard self-closing tags.",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS",
    "html5",
    "Self-closing tags",
    "W3C validation"
  ],
  "version": "12.0.0",
  "extra": {
    "typo3/cms": {
      "extension-key": "html5_patch"
    }
  },
  "require": {
    "typo3/cms-core": "^12.4"
  },
  "require-dev": {
    "cweagans/composer-patches": "^1.7"
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
